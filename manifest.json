{
	"name": "Zen Image Save",
	"description": "Save images without fu*****",
	"version": "1.0.0",
	"manifest_version": 2,
	"content_scripts": [{
		"all_frames": true,
		"exclude_globs": [  ],
		"include_globs": [ "*" ],
		"js": [ "js/lib/jquery-2.1.3.min.js", "js/lib/jquery.nearest.min.js", "js/inject.js" ],
		"matches": [ "<all_urls>" ],
		"run_at": "document_idle"
	}],
	"default_locale": "en",
	"background": {
		"persistent": true,
		"scripts": [ "js/bg.js" ]
	},
	"icons": {
		"16": "images/16.png",
		"48": "images/48.png",
		"128": "images/128.png",
		"256": "images/256.png"
	},
	"options_page": "options.html",
	"permissions": [ "tabs", "downloads", "*://*/*", "storage", "http://*/*", "https://*/*" ]
}
